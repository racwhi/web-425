body{
  font-family: Arial, Helvetica, sans-serif;
  margin: 0;
  padding: 0;
}

.header, .footer{
  background-color: #333;
  color: #fff;
  padding: 10px 0;
}

.header nav , .footer nav{
text-align: center;
}

.header ul, .footer ul{
  list-style-type: none;
  padding: 0;
}


.header li, .footer li{
 display: none;
  margin: 0;
}

.header a, .footer a{
 color: #fff;
  text-decoration: none;
}

.main-content{
  padding: 20px;
}
